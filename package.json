{
  "name": "@spaace/common-lib",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "build": "rm -rf ./dist && tsc",
    "lint": "eslint --fix",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "dev:pubsub": "ts-node ./src/pubsub/init.ts",
    "dev:firestore": "ts-node ./src/database/firestore/init.ts",
    "migration:generate": "npm run typeorm -- migration:generate -d src/database/$npm_config_database src/database/$npm_config_database/migration/$npm_config_name",
    "migration:generate:on-chain": "npm run migration:generate --database=on-chain",
    "migration:generate:off-chain": "npm run migration:generate --database=off-chain"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.4.3",
    "@google-cloud/pubsub": "^3.3.0",
    "@nestjs/graphql": "^10.1.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.8.3",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "typeorm": "0.3.11"
  }
}
